outes = () => {\n    const {login, register, currentUser, currentAddress} = useContext(AppContext)\n    return (\n        <div>\n            <Switch>\n                <Route exact path=\"/login\">\n                    <Login login={login} user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/signup\">\n                    <SignUp register={register} user={currentUser}/>\n                </Route>\n\n                <EnsureLoginRoute exact path=\"/profile\">\n                    <Profile currentUser={currentUser} currentAddress={currentAddress}/>\n                </EnsureLoginRoute>\n\n                <EnsureLoginRoute exact path=\"/profile/edit\">\n                    <ProfileEdit currentUser={currentUser} currentAddress={currentAddress}/>\n                </EnsureLoginRoute>\n\n                <Route exact path=\"/signature-meal\">\n                    <SignatureMeal user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/pair-meal\">\n                    <PairMeal user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/plans&amp;pricing\">\n                    <PriceAndPlans user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/checkout\">\n                    <Checkout user={currentUser} address={currentAddress}/>\n                </Route>\n\n                <Route exact path=\"/policy\">\n                    <Policy/>\n                </Route>\n\n                <Route exact path=\"/\">\n                    <Home/>\n                </Route>\n            \n                <Route exact path=\"/error/must-login-or-signup\">\n                    <ErrorRedirect/>\n                </Route>\n                <Route exact path=\"/access/error\">\n                    <AccessError/>\n                </Route>\n\n                <Route>\n                    <NotFound/>\n                </Route>\n            </Switch>\n        </div>\n    )\n}\n\nexport default Routes","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/AppContext.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/Home.js",["138"],"import React, {useState} from 'react'\nimport {Link} from 'react-router-dom'\nimport HowItWorks from './products/HowItWorks'\nimport Reviews from './products/Reviews'\nimport './styles/Home.css'\n\nconst Home = () => {\n    return (\n        <main className=\"Home global-t-a col-md\">\n            <section className=\"Home-bg col-md\">\n                <div className=\"container\">\n                    <p className=\"Home-spacing\">.</p>\n                        <div className=\"Home-title\">\n                            <p className=\"Home-ourTitle\">Hungry?</p>\n                            <p className=\"Home-p\">Having a hard time thinking of what to eat?</p>\n                        </div>\n                <div>\n                    <p className=\"Home-p\">\n                        Save yourself the headache and have us pick out what to eat for you!\n                    </p>\n                </div>\n                {/* get started button expands when hovered */}\n                <Link className=\"btn btn-danger\" to=\"/plans&amp;pricing\">Get Started!</Link>\n                </div>\n            </section>\n                {/* Have the page scroll to buy or add to cart about pricing? */}\n\n            <section className=\"mt-5\">\n                <HowItWorks/>\n            </section>\n\n            <section className=\"mt-5\">\n                <Reviews/>\n            </section>\n        </main>\n    )\n}\n\nexport default Home","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/EnsureLoginRoute.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/NotFound.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/Login.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/SignUp.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/Profile.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/usersApi.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/commons/LoadSpinner.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useLocalStorage.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/ProfileEdit.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/AccessError.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/PriceAndPlans.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/foodApi.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/HowItWorks.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Reviews.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useFormData.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useError.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/config.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/SignatureMeal.js",["139"],"import React, {useState} from 'react'\nimport parse from 'html-react-parser'\nimport {useHistory, Link} from 'react-router-dom'\nimport Products from './foodApi'\nimport LoadSpinner from '../commons/LoadSpinner'\nimport useLocalStorage from '../custom-hooks/useLocalStorage'\nimport '../styles/SignatureMeal.css'\n\n//get a single random meal by requesting from API and render image and description\nconst SignatureMeal = ({user}) => {\n    const signatureMealStorage = \"signature-meal\"\n    const history = useHistory()\n    const [mealTitle, setMealTitle] = useState(null)\n    const [mealImage, setMealImage] = useState(null)\n    const [mealPrice, setMealPrice] = useState(null)\n    const [mealSummary, setMealSummary] = useState(\"\")\n    const [isRequesting, setIsRequesting] = useState(false)\n    const [signatureMeal, setSignatureMeal] = useLocalStorage(signatureMealStorage)\n\n    //look for payment api\n    //move on to requesting our api when purchased\n    //make user only be able to pick one random meal a day\n    //have a countdown timer when clicked 24hour\n    //css\n\n    //requests random meal timed for loading to show animation (stores in localStorage and must run through as json to store multiple)\n    const getRandomMeal = async() => {\n        const res = await Products.getRandomMeal()\n        const mealResults = await Products.getSignatureMeal()\n        const mealPrice = mealResults.data[0].price\n        const {image, title, summary} = res.data.recipes[0]\n        setTimeout(() => {\n            setMealTitle(title)\n            setMealImage(image)\n            setMealSummary(summary)\n            setMealPrice(mealPrice)\n        }, 4000)\n        setIsRequesting(true)\n    }\n\n    //adds requested API info to localStorage to be able to pass info to Checkout component\n    const addToCart = async () =>{ \n        await setSignatureMeal(JSON.stringify({mealTitle, mealImage, mealPrice}))\n        setTimeout(()=> {\n            history.push('/checkout')\n        }, 500)\n    }\n\n    //setLogin error when redirected if not logged in...\n    //useState change if theres added to cart\n    //We can request the API HERE and users can either continue with purchase or wait till next time. (Have a second random pick?)\n    \n    // if(!user) {\n    //     history.push(\"/error/must-login-or-signup\")\n    // }\n\n    return (\n        <div className=\"Signature-Meal-m global-t-a\">\n            {/* if the request has been filled then render the API(food) */}\n            {mealImage ? \n                <div className=\"Signature-Meal-c card\">\n                    <div className=\"card-body\">\n                        <h1 className=\"Signature-Meal-f card-title\">{mealTitle}</h1>\n                        <div className=\"container\">   \n                        <div className=\"row\">\n                            <div className=\"col-md-3\">\n                            </div>\n                            <div className=\"col-md-6\">\n                            <img className=\"Signature-Meal-i\" src={mealImage} alt=\"Signature Meal\"/>\n                            </div>\n                            <div className=\"col-md-3\">\n                            </div>\n                        </div>\n                            <p>{parse(mealSummary)}</p>\n                        </div>\n                        <button className=\"btn btn-default mt-2\" style={{color: \"white\"}} onClick={addToCart}>Add to Cart</button>\n                    </div>\n                    <p className=\"Signature-Meal-policy\">Not satisfied with this choice? Check out our meal <Link style={{textDecoration: \"none\"}} to=\"/policy\">policy</Link>.</p>\n                </div>\n                :   \n                // add a conditional inside to render loading statement when requesting\n            <div className=\"Signature-Meal-e\" style={{width: \"30rem\"}}>\n                {!isRequesting ?  \n                <div className=\"Signature-Meal-e-c card\">\n                <div className=\"card-body\">\n                <div className=\"row\">\n                    <h1 className=\"Signature-Meal-f mb-4\">The way it works</h1>\n                    <div className=\"col-md-0\">\n                    </div> \n                    <p className=\"Signature-Meal-p-f col-md-12\">\n                            When you're ready, you <b>click the button</b>, we have a system that randomly chooses a meal for you from our wide variety of choices. Foods from all over the world.\n                            The meal is one serving, for one person, at one sitting - <b>just heat and eat.</b>\n                    </p>     \n                    <div className=\"col-md-0\">\n                    </div>\n                    \n                    <div>\n                    <button className=\"Signature-Meal-b btn btn-default w-25\" style={{color: \"white\"}} onClick={getRandomMeal}>Get Meal</button>\n                    </div>\n                </div>\n                </div>\n                </div>\n                : <div>\n                    <LoadSpinner/> \n                    <p className=\"Signature-Meal-p-f m-4\">Please wait while we retrieve your food!</p>\n                </div>\n                }\n                {/* Not satisfied? make a quick policy and make it a modal about 24 hour rule */}\n            </div>\n        }\n\n        </div>\n    )\n}\n\n\nexport default SignatureMeal\n","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/PairMeal.js",["140"],"import React, {useState} from 'react'\nimport {useHistory} from 'react-router-dom'\nimport Products from './foodApi'\nimport shuffleArray from '../commons/shuffleArray'\n\n//User is able to pick this option that requests a random meal paired with a random recommended wine that works well together\nconst PairMeal = ({user}) => {\n    const history = useHistory()\n    const [randMeal, setRandMeal] = useState(null) \n    const [randWine, setRandWine] = useState(null)\n\n    //load state in function? Until request is finished then load the screen. Put a nice loading on it.\n    //look for description for both and combined it.\n    //css\n    const getPairMeal = async() => {\n        const randMeal = await Products.getRandomMeal()\n        const randWine = await Products.getRandomWine()\n        //shuffles through the returned wine list and returns the first value of API array\n        const ourWines = shuffleArray(randWine.data.recommendedWines)\n        const {imageUrl} = ourWines\n        const {image} = randMeal.data.recipes[0]\n        setRandMeal(image)\n        setRandWine(imageUrl)\n        console.log(ourWines)\n    }\n\n    // if(!user) {\n    //     history.push(\"/error/must-login-or-signup\")\n    // }\n\n    return (\n        <div className=\"global-mt\">\n            <button onClick={getPairMeal}>Get Pair Meal</button>\n            <img src={randMeal} alt=\"Random Meal\"/>\n            <img src={randWine} alt=\"Random Meal\"/>\n        </div>\n    )\n}\n\nexport default PairMeal","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/ErrorRedirect.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/commons/shuffleArray.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Policy.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Checkout.js",["141","142"],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","severity":1,"message":"148","line":2,"column":24,"nodeType":"149","messageId":"150","endLine":2,"endColumn":28},{"ruleId":"147","severity":1,"message":"151","line":1,"column":16,"nodeType":"149","messageId":"150","endLine":1,"endColumn":24},{"ruleId":"147","severity":1,"message":"152","line":18,"column":12,"nodeType":"149","messageId":"150","endLine":18,"endColumn":25},{"ruleId":"147","severity":1,"message":"153","line":8,"column":11,"nodeType":"149","messageId":"150","endLine":8,"endColumn":18},{"ruleId":"147","seve