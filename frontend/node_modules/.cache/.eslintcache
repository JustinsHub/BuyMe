unt":0,"fixableWarningCount":0,"usedDeprecatedRules":"68"},{"filePath":"129","messages":"130","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"68"},{"filePath":"131","messages":"132","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"68"},{"filePath":"133","messages":"134","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":"135"},"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/index.js",[],["136","137"],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/reportWebVitals.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/App.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/Navbar.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/AppContextWrapper.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/Footer.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/Routes.js",["138"],"import React, {useContext} from 'react'\nimport {Switch, Route, Link} from 'react-router-dom'\nimport Home from './Home'\nimport Login from './users/Login'\nimport SignUp from './users/SignUp'\nimport NotFound from './NotFound'\nimport AppContext from './AppContext'\nimport EnsureLoginRoute from './EnsureLoginRoute'\nimport Profile from './users/Profile'\nimport ProfileEdit from './users/ProfileEdit'\nimport PriceAndPlans from './products/PriceAndPlans'\nimport SignatureMeal from './products/SignatureMeal'\nimport PairMeal from './products/PairMeal'\nimport Checkout from './products/Checkout'\nimport Policy from './products/Policy'\nimport ErrorRedirect from './users/ErrorRedirect'\nimport AccessError from './users/AccessError'\n\n\n\nconst Routes = () => {\n    const {login, register, currentUser, currentAddress} = useContext(AppContext)\n    return (\n        <div>\n            <Switch>\n                <Route exact path=\"/login\">\n                    <Login login={login} user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/signup\">\n                    <SignUp register={register} user={currentUser}/>\n                </Route>\n\n                <EnsureLoginRoute exact path=\"/profile\">\n                    <Profile currentUser={currentUser} currentAddress={currentAddress}/>\n                </EnsureLoginRoute>\n\n                <EnsureLoginRoute exact path=\"/profile/edit\">\n                    <ProfileEdit currentUser={currentUser} currentAddress={currentAddress}/>\n                </EnsureLoginRoute>\n\n                <Route exact path=\"/signature-meal\">\n                    <SignatureMeal user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/pair-meal\">\n                    <PairMeal user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/plans&amp;pricing\">\n                    <PriceAndPlans user={currentUser}/>\n                </Route>\n\n                <Route exact path=\"/checkout\">\n                    <Checkout user={currentUser} address={currentAddress}/>\n                </Route>\n\n                <Route exact path=\"/policy\">\n                    <Policy/>\n                </Route>\n\n                <Route exact path=\"/\">\n                    <Home/>\n                </Route>\n            \n                <Route exact path=\"/error/must-login-or-signup\">\n                    <ErrorRedirect/>\n                </Route>\n                <Route exact path=\"/access/error\">\n                    <AccessError/>\n                </Route>\n\n                <Route>\n                    <NotFound/>\n                </Route>\n            </Switch>\n        </div>\n    )\n}\n\nexport default Routes","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/AppContext.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/Home.js",["139"],"import React, {useState} from 'react'\nimport {Link} from 'react-router-dom'\nimport HowItWorks from './products/HowItWorks'\nimport Reviews from './products/Reviews'\nimport './styles/Home.css'\n\nconst Home = () => {\n    return (\n        <main className=\"Home global-t-a col-md\">\n            <section className=\"Home-bg col-md\">\n                <div className=\"container\">\n                    <p className=\"Home-spacing\">.</p>\n                        <div className=\"Home-title\">\n                            <p className=\"Home-ourTitle\">Hungry?</p>\n                            <p className=\"Home-p\">Having a hard time thinking of what to eat?</p>\n                        </div>\n                <div>\n                    <p className=\"Home-p\">\n                        Save yourself the headache and have us pick out what to eat for you!\n                    </p>\n                </div>\n                {/* get started button expands when hovered */}\n                <Link className=\"btn btn-danger\" to=\"/plans&amp;pricing\">Get Started!</Link>\n                </div>\n            </section>\n                {/* Have the page scroll to buy or add to cart about pricing? */}\n\n            <section className=\"mt-5\">\n                <HowItWorks/>\n            </section>\n\n            <section className=\"mt-5\">\n                <Reviews/>\n            </section>\n        </main>\n    )\n}\n\nexport default Home","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/EnsureLoginRoute.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/NotFound.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/Login.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/SignUp.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/Profile.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/usersApi.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/commons/LoadSpinner.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useLocalStorage.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/ProfileEdit.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/AccessError.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/PriceAndPlans.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/foodApi.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/HowItWorks.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Reviews.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useFormData.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/custom-hooks/useError.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/config.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/SignatureMeal.js",["140"],"import React, {useState} from 'react'\nimport parse from 'html-react-parser'\nimport {useHistory, Link} from 'react-router-dom'\nimport Products from './foodApi'\nimport LoadSpinner from '../commons/LoadSpinner'\nimport useLocalStorage from '../custom-hooks/useLocalStorage'\nimport '../styles/SignatureMeal.css'\n\n//get a single random meal by requesting from API and render image and description\nconst SignatureMeal = ({user}) => {\n    const signatureMealStorage = \"signature-meal\"\n    const history = useHistory()\n    const [mealTitle, setMealTitle] = useState(null)\n    const [mealImage, setMealImage] = useState(null)\n    const [mealPrice, setMealPrice] = useState(null)\n    const [mealSummary, setMealSummary] = useState(\"\")\n    const [isRequesting, setIsRequesting] = useState(false)\n    const [signatureMeal, setSignatureMeal] = useLocalStorage(signatureMealStorage)\n\n    //look for payment api\n    //move on to requesting our api when purchased\n    //make user only be able to pick one random meal a day\n    //have a countdown timer when clicked 24hour\n    //css\n\n    //requests random meal timed for loading to show animation (stores in localStorage and must run through as json to store multiple)\n    const getRandomMeal = async() => {\n        const res = await Products.getRandomMeal()\n        const mealResults = await Products.getSignatureMeal()\n        const mealPrice = mealResults.data[0].price\n        const {image, title, summary} = res.data.recipes[0]\n        setTimeout(() => {\n            setMealTitle(title)\n            setMealImage(image)\n            setMealSummary(summary)\n            setMealPrice(mealPrice)\n        }, 4000)\n        setIsRequesting(true)\n    }\n\n    //adds requested API info to localStorage to be able to pass info to Checkout component\n    const addToCart = async () =>{ \n        await setSignatureMeal(JSON.stringify({mealTitle, mealImage, mealPrice}))\n        setTimeout(()=> {\n            history.push('/checkout')\n        }, 500)\n    }\n\n    //setLogin error when redirected if not logged in...\n    //useState change if theres added to cart\n    //We can request the API HERE and users can either continue with purchase or wait till next time. (Have a second random pick?)\n    \n    // if(!user) {\n    //     history.push(\"/error/must-login-or-signup\")\n    // }\n\n    return (\n        <div className=\"Signature-Meal-m global-t-a\">\n            {/* if the request has been filled then render the API(food) */}\n            {mealImage ? \n                <div className=\"Signature-Meal-c card\">\n                    <div className=\"card-body\">\n                        <h1 className=\"Signature-Meal-f card-title\">{mealTitle}</h1>\n                        <div className=\"container\">   \n                        <div className=\"row\">\n                            <div className=\"col-md-3\">\n                            </div>\n                            <div className=\"col-md-6\">\n                            <img className=\"Signature-Meal-i\" src={mealImage} alt=\"Signature Meal\"/>\n                            </div>\n                            <div className=\"col-md-3\">\n                            </div>\n                        </div>\n                            <p>{parse(mealSummary)}</p>\n                        </div>\n                        <button className=\"btn btn-default mt-2\" style={{color: \"white\"}} onClick={addToCart}>Add to Cart</button>\n                    </div>\n                    <p className=\"Signature-Meal-policy\">Not satisfied with this choice? Check out our meal <Link style={{textDecoration: \"none\"}} to=\"/policy\">policy</Link>.</p>\n                </div>\n                :   \n                // add a conditional inside to render loading statement when requesting\n            <div className=\"Signature-Meal-e\" style={{width: \"30rem\"}}>\n                {!isRequesting ?  \n                <div className=\"Signature-Meal-e-c card\">\n                <div className=\"card-body\">\n                <div className=\"row\">\n                    <h1 className=\"Signature-Meal-f mb-4\">The way it works</h1>\n                    <div className=\"col-md-0\">\n                    </div> \n                    <p className=\"Signature-Meal-p-f col-md-12\">\n                            When you're ready, you <b>click the button</b>, we have a system that randomly chooses a meal for you from our wide variety of choices. Foods from all over the world.\n                            The meal is one serving, for one person, at one sitting - <b>just heat and eat.</b>\n                    </p>     \n                    <div className=\"col-md-0\">\n                    </div>\n                    \n                    <div>\n                    <button className=\"Signature-Meal-b btn btn-default w-25\" style={{color: \"white\"}} onClick={getRandomMeal}>Get Meal</button>\n                    </div>\n                </div>\n                </div>\n                </div>\n                : <div>\n                    <LoadSpinner/> \n                    <p className=\"Signature-Meal-p-f m-4\">Please wait while we retrieve your food!</p>\n                </div>\n                }\n                {/* Not satisfied? make a quick policy and make it a modal about 24 hour rule */}\n            </div>\n        }\n\n        </div>\n    )\n}\n\n\nexport default SignatureMeal","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/PairMeal.js",["141"],"import React, {useState} from 'react'\nimport {useHistory} from 'react-router-dom'\nimport Products from './foodApi'\nimport shuffleArray from '../commons/shuffleArray'\n\n//User is able to pick this option that requests a random meal paired with a random recommended wine that works well together\nconst PairMeal = ({user}) => {\n    const history = useHistory()\n    const [randMeal, setRandMeal] = useState(null) \n    const [randWine, setRandWine] = useState(null)\n\n    //load state in function? Until request is finished then load the screen. Put a nice loading on it.\n    //look for description for both and combined it.\n    //css\n    const getPairMeal = async() => {\n        const randMeal = await Products.getRandomMeal()\n        const randWine = await Products.getRandomWine()\n        //shuffles through the returned wine list and returns the first value of API array\n        const ourWines = shuffleArray(randWine.data.recommendedWines)\n        const {imageUrl} = ourWines\n        const {image} = randMeal.data.recipes[0]\n        setRandMeal(image)\n        setRandWine(imageUrl)\n        console.log(ourWines)\n    }\n\n    // if(!user) {\n    //     history.push(\"/error/must-login-or-signup\")\n    // }\n\n    return (\n        <div className=\"global-mt\">\n            <button onClick={getPairMeal}>Get Pair Meal</button>\n            <img src={randMeal} alt=\"Random Meal\"/>\n            <img src={randWine} alt=\"Random Meal\"/>\n        </div>\n    )\n}\n\nexport default PairMeal","/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/users/ErrorRedirect.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/commons/shuffleArray.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Policy.js",[],"/Users/anyone/Desktop/Coding/SB Bootcamp/Capstone-Project-2/Pickout/frontend/src/components/products/Checkout.js",["142","143"],"import React, {useState} from 'react'\nimport {Link, NavLink} from 'react-router-dom'\n\nconst Checkout = ({user, address}) => {\n    //after 24 hour use of local storage, remove item.\n    const [localSignatureMeal, setLocalSignatureMeal] = useState(JSON.parse(localStorage.getItem('signature-meal')))\n    //user address for delivery. \n    //if no user address then notify user to update their profile for address.\n    const {first_name, last_name, email} = user.data\n    const {street_address, address_number, city, state, zip_code, country} = address.data\n    \n    return (\n        //use effect the address here so when a user restarts it auto requests it\n        //put this in a card\n        //have the product in the middle?\n        //problem with the changing the address and going back to checkout (put random meal in localStorage when going back to checkout?)\n        //create a cart on navbar for user to go back (css when theres something on the cart?)\n        //conditional on picking a meal when it's already picked revert back to checkout for finalization. (24 hour timer)\n        <div className=\"global-mt\">\n            {street_address === null || \"\" ? \n            <div className=\"card\">\n                <div className=\"card-body\">\n                    <h3>You current don't have a shipping address on your profile. Please <Link className=\"global-link\" to=\"/profile/edit\">update</Link> your address in order to continue checkout.</h3>\n                </div>\n            </div>\n            : \n            <div class=\"container\">\n                <h1 className=\"global-font global-link text-center\">Review Your Order</h1>\n                <div className=\"card\">\n                <div class=\"row\">\n                    <div class=\"col-md-4\">\n                            <div className=\"card-body\">\n                                <h6 className=\"card-subtitle mb-2 text-muted\">Shipping address</h6>\n                                {/* modal change on address? */}\n                                <div>\n                                    <p className=\"global-ct card-text\">{first_name} {last_name}</p>\n                                    <p className=\"global-ct card-text\">{street_address} {address_number}</p>\n                                    <p className=\"global-ct card-text\">{city}, {state} {zip_code}</p>\n                                    <p className=\"global-ct card-text\">{country}</p>\n                                </div>\n\n                                <div className=\"mt-5\">\n                                <h6 className=\"card-subtitle mb-2 text-muted\">Email address</h6>\n                                <p className=\"global-ct\">{email}</p>\n                            </div>\n                            </div>\n                    </div>\n                        <div className=\"mt-2 col-md-4 text-center\">\n                            <h6 className=\"card-subtitle text-muted text-center\">Items on your cart</h6>\n                            {!localSignatureMeal ?\n                            <p><b>Your cart is currently empty</b></p> :\n                            <div>\n                                <div>\n                                    <b>{localSignatureMeal.mealTitle}</b>\n                                </div>\n                                <div>\n                                    <img className=\"col-9 rounded\" src={localSignatureMeal.mealImage} alt=\"signature-meal\"></img>\n                                </div>\n                            </div>\n                            }\n                        </div>\n                    <div class=\"col-md-4 mt-2\">\n                        <div className=\"card-body\">\n                            <div className=\"card\"> \n                                <div className=\"container mt-4 mb-4\">\n                                    <div className=\"row\">\n                                        <div className=\"col-md-6\"> \n                                            <div className=\"d-flex justify-content-start\">                            \n                                            <p className=\"card-subtitle text-muted\">Item(s) total</p> \n                                            </div>\n                                            \n                                            <div className=\"d-flex justify-content-start\">\n                                            <p className=\"card-subtitle text-muted\">Shipping</p> \n                                            </div>\n\n                                            <div className=\"d-flex justify-content-start\">\n                                            <p className=\"card-subtitle text-muted\">Sales tax</p>\n                                            </div>\n                                        </div>\n                                    \n                                        <div className=\"col-md-6\"> \n                                            <div className=\"d-flex justify-content-end\">\n                                                    {!localSignatureMeal ?\n                                                    <p className=\"card-subtitle text-muted\">$0.00</p> :\n                                                    <p className=\"card-subtitle text-muted\">${localSignatureMeal.mealPrice}</p> \n                                                    }\n                                                </div>      \n                                                <div className=\"d-flex justify-content-end\">\n                                                    <p className=\"card-subtitle\" style={{color: \"green\"}}>FREE</p>\n                                                </div> \n                                                <div className=\"d-flex justify-content-end\">   \n                                                    <p className=\"card-subtitle text-muted\">$0.00</p>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <hr></hr>\n                                        <div className=\"row\">\n                                            <div className=\"col-md-6\">\n                                                <p className=\"card-subtitle\"><b>Order total</b></p> \n                                            </div>\n\n                                            <div className=\"col-md-6 d-flex justify-content-end\">\n                                            {!localSignatureMeal ?\n                                                    <p className=\"card-subtitle text-muted\"><b>$0.00</b></p> :\n                                                    <p className=\"card-subtitle text-muted\"><b>${localSignatureMeal.mealPrice}</b></p> \n                                            }\n                                            </div>\n                                        <div>\n                                            {/* if someone tries to make an order either disable or modal your cart is empty */}\n                                            <button className=\"w-100 btn btn-default mt-3\" style={{color: \"white\"}}>Place your order</button>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    <p>Terms and conditions</p>\n                    </div>\n                </div>\n                </div>\n                </div>\n            }\n        </div>\n    )\n}\n\nexport default Checkout",{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","replacedBy":"147"},{"ruleId":"148","severity":1,"message":"149","line":2,"column":24,"nodeType":"150","messageId":"151","endLine":2,"endColumn":28},{"ruleId":"148","severity":1,"message":"152","line":1,"column":16,"nodeType":"150","messageId":"151","endLine":1,"endColumn":24},{"ruleId":"148","severity":1,"message":"153","line":18,"column":12,"nodeType":"150","messageId":"151","endLine":18,"endColumn":25},{"ruleId":"148","severity":1,"message":"154","line":8,"column":11,"nodeType":"150","messageId":"151","endLine":8,"endColumn":18},{"ruleId":"148","severity":1,"message":"155","line":2,"column":15,"nodeType":"150","messageId":"151","endLine":2,"endColumn":22},{"ruleId":"148","severity":1,"message":"156","line":6,"column":32,"nodeType":"150","messageId":"151","endLine":6,"endColumn":53},"no-native-reassign",["157"],"no-negated-in